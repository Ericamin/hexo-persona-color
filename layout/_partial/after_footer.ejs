<!--[if lt IE 9]><script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8/jquery.min.js"></script>
<script>
  $(document).ready(function() {
    $('#sidebar').delay(600).animate({ width: '285px' }, 500, function() {
      $('.personal-info').css({display: 'table'}).hide().fadeIn(500);
      $('.theme-info').css({display: 'table'}).hide().fadeIn(500);
    });
  });
</script>
<script>
  $(document).ready(function() {
    $('#header').delay(600).animate({ height: '75px' }, 500, function() {
      $('.personal-info-mini').css({display: 'inline'}).hide().fadeIn(500);
    });
  });
</script>
<script src="<%- config.root %>js/headroom.min.js"></script>
<script src="<%- config.root %>js/jQuery.headroom.min.js"></script>

<script>
$("#header").headroom({
  "tolerance": 5,
  "offset": 205,
  "classes": {
    "initial": "",
    "pinned": "slideInDown",
    "unpinned": "slideOutUp",
  }
});
$("#header").headroom("destroy");
</script>

<% if (config.disqus_shortname){ %>
<script type="text/javascript">
var disqus_shortname = '<%= config.disqus_shortname %>';
(function(){
  var dsq = document.createElement('script');
  dsq.type = 'text/javascript';
  dsq.async = true;
  dsq.src = '//' + disqus_shortname + '.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';
  (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
}());
</script>
<% } %>

<%- partial('google_analytics') %>
<script src="<%- config.root %>js/jquery.imagesloaded.min.js"></script>
<script src="<%- config.root %>js/gallery.js"></script>

<% if (theme.fancybox){ %>
<link rel="stylesheet" href="<%- config.root %>fancybox/jquery.fancybox.css" media="screen" type="text/css">
<script src="<%- config.root %>fancybox/jquery.fancybox.pack.js"></script>
<script type="text/javascript">
(function($){
  $('.fancybox').fancybox();
})(jQuery);
</script>
<% } %>
